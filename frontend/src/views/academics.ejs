    <section class="admission_cources">
      <div class="container">
        <div class="row text-center">
          <div class="col-md-12">
            <h2>Our Courses</h2>
            <p>
              Lorem Ipsum is simply dummy text of the printing and typesetting
              industry. Lorem Ipsum has been the industry's standard dummy text
              ever since the 1500s, when an unknown printer took a galley of
              type and scrambled
            </p>
          </div>
        </div>
        <div class="row">
            <% courses.forEach(function(course, index){%>
          <div class="col-md-6">
            <a href="course-detail.html" class="course_box">
              <img
                src="<%- course.attributes.course_image.data.attributes.url %>"
                class="img-fluid"
                alt="#"
              />
              <div class="couse-desc-wrap">
                <h4><%- course.attributes.course_title %></h4>
                <!-- // end .star-rating -->
                <p>
                    <%- course.attributes.short_description %>
                </p>
              </div>
              <!-- // end .couse-desc-wrap -->
              <div class="course_duration">
                <ul>
                  <li>
                    <p>Duration</p>
                    <span><%- course.attributes.course_duration %> Year</span>
                  </li>
                  <li>
                    <p>Fee</p>
                    <span>Shs.<%-course.attributes.course_price %></span>
                  </li>
                </ul>
              </div>
            </a>
          </div>
          <% }) %>
        </div>
      </div>
    </section>
