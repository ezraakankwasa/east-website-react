    <section class="our_courses">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <h2>Our Courses</h2>
          </div>
        </div>
        <div class="row">
          <% courses.forEach(function(course, index){%>
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3">
              <div class="courses_box mb-5">
                <div class="course-img-wrap">
                  <img
                    src="<%- course.attributes.course_image.data.attributes.url %>"
                    class="img-fluid"
                    alt="courses-img"
                  />
                  <div class="courses_box-img">
                    <div class="courses-link-wrap">
                      <a href="/academics/<%- course.id %>" class="course-link"
                        ><span>course Details </span></a
                      >
                      <a href="/admission" class="course-link"
                        ><span>Join today </span></a
                      >
                    </div>
                    <!-- // end .courses-link-wrap -->
                  </div>
                </div>
                <!-- // end .course-img-wrap -->
                <div class="courses_icon">
                  <img
                    src="images/plus-icon.png"
                    class="img-fluid close-icon"
                    alt="plus-icon"
                  />
                </div>
                <a href="/academics/<%- course.id %>" class="course-box-content">
                  <h3><%- course.attributes.course_title %></h3>
                  <p>
                    <%- course.attributes.short_description %>
                  </p>
                </a>
              </div>
            </div>
          <% }) %>
          <!--End of where to delete-->
        </div>
        <div class="row">
          <div class="col-md-12 text-center">
            <a href="/academics" class="btn btn-default btn-courses">View all courses</a>
          </div>
        </div>
      </div>
    </section>
