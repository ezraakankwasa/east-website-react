    <section class="our-teachers">
      <div class="container">
        <div class="row">
            <% teachers.forEach(function(teacher, index){%>
          <div class="col-xs-12 col-sm-6 col-md-6">
            <div class="our-teachers-block">
              <a href="teacher/<%- teacher.id %>" class="lecturer-link">
              <img
                src=<%- teacher.attributes.lecturer_image.data.attributes.url %>
                class="img-fluid teachers-img"
                alt="#"
              />
            </a>
              <div class="teachers-description">
                <p>
                  <strong><%- teacher.attributes.lecturer_name %></strong> <br />
                  <%- teacher.attributes.lecturer_qualification %>
                </p>
                <hr />
                <p>
                  Syllabus : <span>Economics, Marketing &amp; Finance</span>
                </p>
